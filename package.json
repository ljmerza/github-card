{
    "name": "github-card",
    "version": "1.2.2",
    "description": "A Github card for Home Assistant Lovelace UI",
    "keywords": [
        "home-assistant",
        "homeassistant",
        "hass",
        "automation",
        "lovelace",
        "Github",
        "custom-cards"
    ],
    "main": "main.js",
    "module": "main.js",
    "repository": "git@github.com:ljmerza/github-card.git",
    "author": "Leonardo Merza <ljmerza@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@babel/polyfill": "^7.4.3",
        "lit-element": "^2.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/plugin-transform-modules-umd": "^7.2.0",
        "@babel/preset-env": "^7.4.3",
        "babel-loader": "^8.0.5",
        "eslint": "^5.13.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.16.0",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-merge": "^4.2.1"
    },
    "scripts": {
        "webpack": "npx webpack --config webpack.config.js",
        "lint": "eslint ./src",
        "start": "webpack --watch --config webpack/webpack.dev.js",
        "build": "npm run lint && npm run build:prod && npm run build:prodbabel",
        "build:prod": "npm run webpack --config webpack/webpack.prod.js",
        "build:prodbabel": "npm run webpack --config webpack/webpack.prod-babel.js"
    }
}
